    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script src="https://cdn.datatables.net/1.10.24/js/jquery.dataTables.min.js"></script>

    <script>


        $(".desutj").css("display", "none");
        $('#example').DataTable({
            'paging': true,
            'lengthChange': true,
            'searching': true,
            "order": [],
            'info': true,
            'autoWidth': true,
            'columnDefs': [{
                'targets': [4],
                'orderable': false,
            }]
        })

        $('#example1').DataTable({
