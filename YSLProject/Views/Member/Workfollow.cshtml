                @Html.EditorFor(model => model.MedicaidID, new { htmlAttributes = new { @class = "form-control", @placeholder = "", @readonly = "readonly" } })
            </li>
            <li>
                <label style="margin-bottom: 0px;">Enrollment Date</label>
                @Html.EditorFor(model => model.EnrollmentDa, new { htmlAttributes = new { @class = "form-control", @placeholder = "", @readonly = "readonly" } })
            </li>
            <li>
                <label style="margin-bottom: 0px;">Disenrolment Date</label>
                @Html.EditorFor(model => model.DisenrolmentDa, new { htmlAttributes = new { @class = "form-control", @placeholder = "", @readonly = "readonly" } })
            </li>
            <li>
